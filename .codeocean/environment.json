{
	"version": 1,
	"base_image": "codeocean/mambaforge3:24.5.0-0-python3.12.4-ubuntu22.04",
	"args": [
		"AWS_ACCESS_KEY_ID",
		"AWS_DEFAULT_REGION",
		"AWS_SECRET_ACCESS_KEY"
	],
	"post_install": true,
	"options": {
		"registry_host_arg": true,
		"git_ask_pass": true
	},
	"installers": {
		"pip": {
			"packages": [
				{
					"name": "aind-data-access-api[docdb]",
					"version": "1.2.2"
				},
				{
					"name": "numpy",
					"version": "1.26.4"
				},
				{
					"name": "pandas",
					"version": "2.1.4"
				},
				{
					"name": "pydantic-settings",
					"version": "2.9.1"
				},
				{
					"name": "s3fs",
					"version": "2025.2.0"
				}
			],
			"options": {},
			"pre_install_options": {}
		}
	},
	"env_variables": [
		{
			"name": "DOCDB_HOST",
			"value": "api.allenneuraldynamics-test.org"
		},
		{
			"name": "DOCDB_DATABASE",
			"value": "analysis"
		},
		{
			"name": "DOCDB_COLLECTION",
			"value": "processing_v2"
		}
	]
}